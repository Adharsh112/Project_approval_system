<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alogin</servlet-name>
        <servlet-class>alogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hod</servlet-name>
        <servlet-class>hod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pic</servlet-name>
        <servlet-class>pic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ig</servlet-name>
        <servlet-class>ig</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>students</servlet-name>
        <servlet-class>students</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Projects</servlet-name>
        <servlet-class>Projects</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alogin</servlet-name>
        <url-pattern>/alogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hod</servlet-name>
        <url-pattern>/hod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pic</servlet-name>
        <url-pattern>/pic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ig</servlet-name>
        <url-pattern>/ig</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>students</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Projects</servlet-name>
        <url-pattern>/Projects</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
